{
  "name": "var-name",
  "displayName": "Var Name",
  "description": "Generate variable names using AI models",
  "version": "0.0.4",
  "private": true,
  "icon": "images/logo.png",
  "publisher": "w2xi",
  "repository": {
    "url": "https://github.com/w2xi/var-name"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:var-name.translate"
  ],
  "main": "./out/extension.js",
  "l10n": "./l10n",
  "contributes": {
    "commands": [
      {
        "command": "var-name.translate",
        "title": "%var-name.translate.title%"
      }
    ],
    "keybindings": [
      {
        "command": "var-name.translate",
        "key": "ctrl+shift+t",
        "mac": "cmd+shift+t"
      }
    ],
    "configuration": {
      "title": "Var Name",
      "properties": {
        "var-name.apiKey": {
          "type": "string",
          "description": "%var-name.apiKey.description%"
        },
        "var-name.baseURL": {
          "type": "string",
          "default": "https://api.deepseek.com/v1/chat/completions",
          "description": "%var-name.baseURL.description%"
        },
        "var-name.model": {
          "type": "string",
          "default": "deepseek-chat",
          "description": "%var-name.model.description%"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "release": "bumpp --commit --push --tag && npx vsce publish",
    "l10n": "vscode-l10n-dev export ./src",
    "l10n:watch": "vscode-l10n-dev watch ./src"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@vscode/l10n": "^0.0.18",
    "@vscode/l10n-dev": "^0.0.35",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bumpp": "^10.1.1"
  }
}
